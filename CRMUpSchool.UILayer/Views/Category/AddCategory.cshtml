@model CRMUpSchool.EntityLayer.Concrete.Category

@{
    ViewData["Title"] = "AddCategory";
    Layout = "~/Views/Shared/_CRMLayout.cshtml";
}

<h1>Kategori Ekle</h1>

<form method="post">
    <label>Kategori Adı:</label>
    <input type="text" asp-for="CategoryName" class="form-control"/>
    <br />
    <label>Kategori Açıklama:</label>
    <input type="text" asp-for="CategoryDescription" class="form-control" />
    <br />
    <button class="btn btn-success">Kategori Kaydet</button>
</form>

